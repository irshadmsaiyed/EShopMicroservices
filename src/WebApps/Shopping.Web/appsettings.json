{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.AspNetCore": "Warning",
        "System": "Warning"
      }
    },
    "Enrich": [
      "FromLogContext",
      "WithMachineName",
      "WithEnvironmentName"
    ],
    "Properties": {
      "Application": "MS-Gateway.API"
    }
  },
  "Elastic": {
    "Uri": "http://localhost:9200",
    "DataStream": {
      "Type": "logs",
      "Dataset": "basket-api",
      "Namespace": "dev"
    }
  },
  "AllowedHosts": "*",
  "ApiSettings": {
    "GatewayAddress": "https://localhost:6064"
  }
}